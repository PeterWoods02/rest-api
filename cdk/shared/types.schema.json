{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
      "Team": {
        "type": "object",
        "properties": {
          "id": { "type": "number" },
          "teamName": { "type": "string" },
          "country": { "type": "string" },
          "league": { "type": "string" },
          "location": { "type": "string" }
        },
        "required": ["id", "teamName", "country", "league", "location"],
        "additionalProperties": false
      }
    }
  }
  
{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "Team": {
            "additionalProperties": false,
            "properties": {
                "country": {
                    "type": "string"
                },
                "founded": {
                    "type": "number"
                },
                "id": {
                    "type": "number"
                },
                "isActive": {
                    "type": "boolean"
                },
                "league": {
                    "type": "string"
                },
                "location": {
                    "type": "string"
                },
                "stadium": {
                    "type": "string"
                },
                "teamName": {
                    "type": "string"
                },
                "titlesWon": {
                    "type": "number"
                }
            },
            "required": [
                "country",
                "founded",
                "id",
                "isActive",
                "league",
                "location",
                "stadium",
                "teamName",
                "titlesWon"
            ],
            "type": "object"
        }
    }
}

